# Test Environment Variables
# Copy this file to .env.test.local and fill in your test values

# Test User Credentials (for E2E tests)
TEST_USER_EMAIL=test.user@terragon.test
TEST_USER_PASSWORD=TestPassword123!

# Base URL for tests
PLAYWRIGHT_BASE_URL=http://localhost:3000

# Supabase Test Configuration
NEXT_PUBLIC_SUPABASE_URL=your_test_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_test_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_test_supabase_service_role_key
NEXT_PUBLIC_SUPABASE_PROJECT_ID=your_test_project_id

# Gmail API Test Configuration
GOOGLE_CLIENT_ID=your_test_google_client_id
GOOGLE_CLIENT_SECRET=your_test_google_client_secret
GOOGLE_REFRESH_TOKEN=your_test_refresh_token

# Clerk Test Configuration
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_test_clerk_publishable_key
CLERK_SECRET_KEY=your_test_clerk_secret_key

# Other API Keys (use test/sandbox versions)
ANTHROPIC_API_KEY=your_test_anthropic_api_key
UPSTASH_REDIS_REST_URL=your_test_upstash_url
UPSTASH_REDIS_REST_TOKEN=your_test_upstash_token

# Test Environment Identifier
NODE_ENV=test