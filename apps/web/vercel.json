{
  "framework": "nextjs",
  "installCommand": "pnpm install --no-frozen-lockfile --registry https://r.cnpmjs.org",
  "buildCommand": "cd ../.. && pnpm run build --filter=web",
  "outputDirectory": ".next",
  "devCommand": "pnpm run dev",
  "crons": [
    {
      "path": "/api/cron/process-emails",
      "schedule": "0 9 * * *"
    }
  ]
}